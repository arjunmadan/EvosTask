<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
                           http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:property-placeholder location="file:///${IRIS_CONFIG_DIRECTORY}/common.properties"/>

    <import resource="classpath:iris-readslave-datasource.xml"/>
    <import resource="classpath:iris-datasource.xml"/>
    <import resource="classpath:order-history-datasource.xml"/>

    <context:component-scan base-package="com.inetwork.jasper.disconnect"/>
    <context:component-scan base-package="com.inetwork.jasper.ldap"/>
    <bean class="com.inetwork.jasper.IrisCustomJRQueryExecuterFactoryBundle"/>
    <bean class="com.inetwork.legacyhistorycomponents.dao.MariaDisconnectTelephoneNumberOrderResponseDao"/>
    <bean class="com.inetwork.legacyhistorycomponents.dao.MariaTelephoneNumberDao"/>
    <bean class="com.inetwork.common.dao.typedorder.TypedOrderDao"/>
    <bean class="com.inetwork.common.dao.account.AccountDao"/>
    <bean class="com.inetwork.common.dao.OrderCompleteCountDao"/>
    <bean class="com.inetwork.common.dao.TierDao"/>
    <bean class="com.inetwork.common.dao.notification.CallbackCredentialsDao"/>
</beans>


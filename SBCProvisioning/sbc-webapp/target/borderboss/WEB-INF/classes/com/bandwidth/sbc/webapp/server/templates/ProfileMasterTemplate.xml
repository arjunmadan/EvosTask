                <Validation type="required" />
            </Validations>
        </Parameter>

        <Parameter id="" type="select" displayName="" readonly="" defaultValue="" description=""  >
                    <Options>
                            <Value default="true" value="v1" />
                            <Value value="v2" />
                    </Options>
        </Parameter>
    -->
    </Entity>
</Template>
